{
  "name": "s3-proxy-stream",
  "description": "Simplified s3.getObject() streams",
  "version": "0.0.0",
  "author": "Jonathan Ong <me@jongleberry.com> (http://jongleberry.com)",
  "license": "MIT",
  "repository": "jonathanong/s3-proxy-stream",
  "dependencies": {
